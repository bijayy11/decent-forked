{
  "name": "Deerhack",
  "version": "1.0.0",
  "description": "This is the project for the Deerhack hackathon",
  "manifest_version": 3,
  "name": "Decent",
  "version": "0.0.1",
  "description": "A simple Chrome extension that encrypts the text of the webpage",
  "action": {
    "default_popup": "./src/components/popup/index.html",
    "default_icon": "./public/assets/Decent.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage",
    "scripting",
    "downloads"
  ],
  "icons": {
    "128": "./public/assets/Decent.png"
  }
}
